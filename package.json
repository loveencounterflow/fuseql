{
  "name": "fuseql",
  "version": "1.0.2",
  "description": "a FUSE virtual filesystem backed by an SQLite database",
  "main": "lib/main.js",
  "scripts": {
    "build": "gulp --harmony build",
    "start": "node --harmony --harmony-proxies lib/main.js",
    "do": "npm run build && node --harmony --harmony-proxies $1",
    "test": "npm run build && node --harmony --harmony-proxies lib/tests.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cnd": "^1.0.8",
    "coffeenode-suspend": "^0.1.11",
    "jsondown": "^0.1.1",
    "level-filesystem": "^1.1.0",
    "level-writestream": "^0.1.3",
    "leveldown": "^1.4.1",
    "levelup": "^1.2.1",
    "memdown": "^1.0.0",
    "sqldown": "^2.1.0",
    "sqlite3": "^3.0.10"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-coffee": "^2.3.1",
    "gulp-sourcemaps": "^1.5.2",
    "guy-test": "^1.0.5",
    "temp": "^0.8.3"
  }
}
