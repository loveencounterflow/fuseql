{
  "name": "fuseql",
  "version": "1.0.2",
  "description": "a FUSE virtual filesystem backed by an SQLite database",
  "main": "lib/main.js",
  "scripts": {
    "build": "gulp --harmony build",
    "start": "node --harmony --harmony-proxies lib/main.js",
    "do": "npm run build && node --harmony --harmony-proxies $1",
    "test": "npm run build && node --harmony --harmony-proxies lib/tests.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "abstract-leveldown": "^2.4.0",
    "cnd": "^1.0.8",
    "fuse4js": "^0.1.12",
    "sqlite3": "^3.0.10"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-coffee": "^2.3.1",
    "gulp-sourcemaps": "^1.5.2",
    "levelup": "^1.2.1"
  }
}
