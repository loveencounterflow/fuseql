  "dependencies": {
    "cnd": "^1.0.8",
    "coffeenode-suspend": "^0.1.11",
    "hollerith": "^1.0.2"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-coffee": "^2.3.1",
    "gulp-sourcemaps": "^1.5.2"
  }
}
